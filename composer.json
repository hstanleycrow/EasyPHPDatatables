{
    "name": "hstanleycrow/easyphpdatatables",
    "version": "1.0.1",
    "description": "Free PHP library web component for datatables and PHP",
    "keywords": [
        "php",
        "components",
        "datatables",
        "web components"
    ],
    "license": "MIT",
    "homepage": "https://github.com/hstanleycrow/EasyPHPDatatables",
    "authors": [
        {
            "name": "Harold Rivas AKA Harold Crow",
            "email": "holamundo@microaplicaciones.com",
            "homepage": "https://www.microaplicaciones.com/",
            "role": "Developer"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/hstanleycrow/EasyPHPWebComponents.git"
        }
    ],
    "require": {
        "php": "^8.2"
    },
    "autoload": {
        "psr-4": {
            "hstanleycrow\\EasyPHPDatatables\\": "src/"
        },
        "files": [
            "src/config.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "hstanleycrow\\DatatablesDefinitions\\": "DatatablesDefinitions/",
            "hstanleycrow\\Components\\": "Components/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "require-dev": {
        "vlucas/phpdotenv": "^5.6",
        "hstanleycrow/easyphpwebcomponents": "^1.0.0",
        "phpunit/phpunit": "^10.4"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}